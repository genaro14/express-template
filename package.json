{
  "name": "express-starter",
  "version": "1.1.0",
  "description": "Express Starter Template",
  "main": "index.js",
  "scripts": {
    "dev": "node --watch src/index.js",
    "lint": "eslint .",
    "test": "pnpx jest",
    "prepare": "husky",
    "release": "standard-version && git push --follow-tags --no-verify",
    "pre-commit": "pnpm run lint && npm run test",
    "pre-push": "pnpm run release"
  },
  "keywords": [
    "template"
  ],
  "author": "GP",
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^9.17.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0",
    "standard-version": "^9.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "chalk": "^4.1.2",
    "morgan": "^1.10.0"
  }
}
